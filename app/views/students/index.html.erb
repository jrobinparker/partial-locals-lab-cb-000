<h1>Student Search</h1>
<%= form_tag(students_path method: :get) do %>
  Student Name <%= text_field_tag :search, params[:search] %>
  <%= submit_tag 'Search' %>
<% end %>

<h1>Search Results</h1>

<% @students.each do |student| %>
    <%= render :partial => "student", collection: @students, as: :student, locals: {student: Student.search(params[:name])} %>
<% end %>
